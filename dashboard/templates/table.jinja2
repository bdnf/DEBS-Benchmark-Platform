{% extends 'base.jinja2' %}

{% block content %}

<table id="myTable">

   <tr>
     <th>Team Name</th>
     <th onclick="sortTable(1)">accuracy</th>
     <th onclick="sortTable(2)">precision</th>
     <th onclick="sortTable(3)" >recall</th>
     <th onclick="sortTable(4)">runtime</th>
   </tr>

   {% for key in post.items() %}
   <tr>
      <td>{{ key[0] }}</td>
      <td>{{ key[1]['accuracy'] }}</td>
      <td>{{ key[1]['precision'] }}</td>
      <td>{{ key[1]['recall'] }}</td>
      <td>{{ key[1]['runtime'] }}</td>

   </tr>
  {% endfor %}

   <!-- {% for user in post.keys() %}
   <tr>
       {% for key in user %}
       <td>{{ user[key] }}</td>
       {% endfor %}
   </tr>
   {% endfor %} -->
</table>

{% endblock %}
